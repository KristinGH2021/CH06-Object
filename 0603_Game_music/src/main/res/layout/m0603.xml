<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Black"
    android:padding="@dimen/p10"
    tools:context=".M0603">

    <TextView
        android:id="@+id/m0603_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:background="@color/Yellow"
        android:gravity="center"
        android:text="@string/m0603_t001"
        android:textColor="@color/Fuchsia"
        android:textSize="@dimen/p30"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0603_t002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_t001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/Aqua"
        android:text="@string/m0603_t002"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/m0603_t003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_t001"
        android:layout_alignEnd="@+id/m0603_t001"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="39dp"
        android:background="@color/Aqua"
        android:text="@string/m0603_t003"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/m0603_b001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_t003"
        android:layout_alignEnd="@+id/m0603_t003"
        android:layout_marginTop="@dimen/p10"
        android:layout_marginEnd="0dp"
        android:background="#00000000"
        android:src="@drawable/scissors"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0603_b002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_b001"
        android:layout_alignEnd="@+id/m0603_b001"
        android:layout_marginTop="@dimen/p10"
        android:layout_marginEnd="0dp"
        android:background="#00000000"
        android:src="@drawable/stone"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/m0603_b003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_b002"
        android:layout_alignEnd="@+id/m0603_b002"
        android:layout_marginTop="@dimen/p10"
        android:layout_marginEnd="0dp"
        android:background="#00000000"
        android:src="@drawable/net"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/m0603_c001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/m0603_t002"
        android:layout_alignTop="@+id/m0603_b002"
        android:layout_marginStart="@dimen/p10"
        android:layout_marginTop="0dp"
        android:background="#00000000"
        android:gravity="center"
        android:textColor="@color/Black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        app:srcCompat="@drawable/scissors" />

    <TextView
        android:id="@+id/m0603_s001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_b003"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:background="@color/Silver"
        android:text="@string/m0603_s001"
        android:textColor="@color/Red"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0603_f000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0603_s001"
        android:layout_alignParentStart="true"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:background="@color/White"
        android:text="@string/m0603_f000"
        android:textColor="@color/black"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/p10"
        android:layout_marginBottom="@dimen/p10"
        android:text="@string/myname"
        android:textColor="@color/White"
        android:textSize="@dimen/fontsize"
        android:textStyle="bold" />

</RelativeLayout>