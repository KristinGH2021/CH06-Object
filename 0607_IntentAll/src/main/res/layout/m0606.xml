<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back01"
    android:padding="@dimen/p20"
    tools:context=".M0606">

    <TextView
        android:id="@+id/m0606_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Yellow"
        android:gravity="center"
        android:text="@string/m0606_t001"
        android:textColor="@color/Red"
        android:textSize="@dimen/Headfont"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/m0606_t002"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_t001"
        android:layout_alignParentStart="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="@dimen/top"
        android:background="@color/Gray"
        android:gravity="center"
        android:text="@string/m0606_t002"
        android:textColor="@color/Blue"
        android:textSize="@dimen/fontsize01"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0606_t003"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/m0606_t002"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/end"
        android:background="@color/Aqua"
        android:gravity="center"
        android:text="@string/m0606_t003"
        android:textColor="@color/Blue"
        android:textSize="@dimen/fontsize01" />

    <ImageButton
        android:id="@+id/m0606_b001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_t003"
        android:layout_alignEnd="@+id/m0606_t003"
        android:layout_marginTop="@dimen/top"
        android:layout_marginEnd="@dimen/p10"
        android:background="#00000000"
        android:src="@drawable/scissors" />

    <ImageButton
        android:id="@+id/m0606_b002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_b001"
        android:layout_alignEnd="@+id/m0606_b001"
        android:layout_marginTop="@dimen/top"
        android:background="#00000000"
        android:src="@drawable/stone" />

    <ImageButton
        android:id="@+id/m0606_b003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_b002"
        android:layout_alignEnd="@+id/m0606_b002"
        android:layout_marginTop="@dimen/top"
        android:background="#00000000"
        android:src="@drawable/net" />

    <ImageView
        android:id="@+id/m0606_c001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/m0606_t002"
        android:layout_alignTop="@+id/m0606_b002"
        android:layout_marginStart="10dp"
        android:layout_marginTop="-1dp"
        android:background="#00000000"
        app:srcCompat="@drawable/scissors" />

    <TextView
        android:id="@+id/m0606_s001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_b003"
        android:layout_marginTop="@dimen/top"
        android:background="@color/Aqua"
        android:text="@string/m0606_s001"
        android:textColor="@color/Navy"
        android:textSize="@dimen/fontsize01"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/m0606_f000"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_s001"
        android:layout_marginTop="@dimen/top"
        android:background="@color/White"
        android:text="@string/m0606_f000"
        android:textSize="@dimen/fontsize01" />

    <TextView
        android:id="@+id/m0606_t006"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m0606_f000"
        android:layout_alignEnd="@+id/m0606_f000"
        android:layout_marginTop="@dimen/p50"
        android:background="@color/Black"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize01" />

</RelativeLayout>